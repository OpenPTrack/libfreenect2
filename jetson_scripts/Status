#!/bin/bash

echo ""
echo "    CPUs active (it should be 0-3):"
echo "$(cat /sys/devices/system/cpu/online)"
echo ""
echo "    CPUs freq (it should be 2032500):"
echo "$(cat /sys/devices/system/cpu/cpu0/cpufreq/scaling_cur_freq)"
echo "$(cat /sys/devices/system/cpu/cpu1/cpufreq/scaling_cur_freq)"
echo "$(cat /sys/devices/system/cpu/cpu2/cpufreq/scaling_cur_freq)"
echo "$(cat /sys/devices/system/cpu/cpu3/cpufreq/scaling_cur_freq)"
echo "    CPU governor (it should be userspace):"
echo "$(cat /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor)"
echo "$(cat /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor)"
echo "$(cat /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor)"
echo "$(cat /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor)"
echo ""
echo "    GPU clock (it should be 852000000):"
echo "$(cat /sys/kernel/debug/clock/override.gbus/rate)"
echo ""
echo "    GPU memory clock (it should be 924000000):"
echo "$(cat /sys/kernel/debug/clock/override.emc/rate)"
echo ""
echo "    USB Autosuspending state (they all should be -1):"
echo "$(grep . /sys/bus/usb/devices/*/power/autosuspend)"
echo ""
